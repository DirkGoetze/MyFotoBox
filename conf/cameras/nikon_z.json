{
    "name": "Nikon Z-Serie",
    "description": "Optimierte Einstellungen f√ºr Nikon Z-Serie Spiegellose Kameras (Z50, Z5, Z6, Z7)",
    "type": "dslm",
    "interface": "gphoto2",
    "detection": {
        "method": "brand_model",
        "brand": "Nikon",
        "model": "Z*"
    },
    "settings": {
        "resolution": {
            "width": 8256,
            "height": 5504
        },
        "aperture": "f/5.6",
        "iso": "200",
        "shutter_speed": "1/125",
        "focus": "auto",
        "white_balance": "auto",
        "compression": "high"
    },
    "preview": {
        "enabled": true,
        "resolution": {
            "width": 1920,
            "height": 1080
        },
        "update_interval": 500
    },
    "defaults": {
        "create_thumbnail": true,
        "save_directory": "photos/nikon",
        "file_format": "jpg"
    },
    "advanced": {
        "camera_config": {
            "viewfinder": 1,
            "autofocus": 1,
            "picture_control": "standard",
            "capture_format": "jpeg",
            "vibration_reduction": "on"
        },
        "usb_settings": {
            "transfer_mode": "PTP"
        }
    }
}
